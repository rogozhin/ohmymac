# git
alias gs='git status'
alias ga='git add '
alias gc='git commit '
alias gca='git commit -am '
alias gco='git checkout '
function gnew() {
    git checkout main
    git pull
    git checkout -b $1
}
function gmerge() {
    local CURR_BR=`git rev-parse --abbrev-ref HEAD`
    git checkout main
    git pull
    git checkout $CURR_BR
    git merge main
}
function gp() {
    if git name-rev @{u} > /dev/null 2>&1; then
      git push
    else
      git push --set-upstream origin $(get_git_branch)
    fi
}
